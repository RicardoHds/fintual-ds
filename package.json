  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.6",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "@types/react-native": "^0.73.0",
    "concurrently": "^9.2.1",
    "typedoc": "^0.28.14",
    "typescript": "^5.9.3"
